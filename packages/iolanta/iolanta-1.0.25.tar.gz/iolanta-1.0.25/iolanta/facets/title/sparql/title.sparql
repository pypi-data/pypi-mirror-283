SELECT * WHERE {
    OPTIONAL {
        $iri rdfs:label ?rdfs_label .
    }

    OPTIONAL {
        $iri <http://purl.org/dc/elements/1.1/title> ?dc_title .
    }

    OPTIONAL {
        $iri <http://schema.org/title> ?schema_title .
    }

    OPTIONAL {
        $iri <http://xmlns.com/foaf/0.1/name> ?foaf_name .
    }
}