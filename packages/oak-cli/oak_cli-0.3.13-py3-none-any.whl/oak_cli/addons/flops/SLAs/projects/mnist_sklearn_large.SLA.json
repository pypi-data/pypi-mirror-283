{
    "verbose": true,
    "customerID": "Admin",
    "ml_repo_url": "https://github.com/Malyuk-A/flops_ml_repo_mnist_sklearn",
    "ml_model_flavor": "sklearn",
    "use_devel_base_images": true,
    "training_configuration": {
        "data_tags": ["mnist"],
