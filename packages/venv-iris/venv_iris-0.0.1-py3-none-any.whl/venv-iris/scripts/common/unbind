#!__VENV_PYTHON__
# -*- coding: utf-8 -*-
# Python script to modify :
# - PythonRuntimeLibrary
# - PythonPath
# in iris
# This script is generated by the venv-iris package
# Do not modify it manually

import iris

iris.system.Process.SetNamespace("%SYS")
config = iris.cls("Config.config").Open()

# Set the new libpython path
config.PythonRuntimeLibrary = ""
# Set the new PythonPath to VENV_DIR
config.PythonPath = ""

config._Save()