{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: densify linestrings",
            "type": "python",
            "request": "launch",
            "module": "geodense.main",
            "args": [
                "densify",
                "-",
                "${workspaceFolder}/out-data/feature.json",
                "-o",
                "<",
                "${workspaceFolder}/linestrings_4326.json",
                "-v"
            ],
            "justMyCode": false
        },
        {
            "name": "Python: densify multipolygon",
            "type": "python",
            "request": "launch",
            "module": "geodense.main",
            "args": [
                "densify",
                "${workspaceFolder}/tests/data/multipolygon.json",
                "${workspaceFolder}/out-data/out.json",
                "--max-segment-length",
                "5000",
                "-o"
            ],
            "justMyCode": false
        },
        {
            "name": "Python: densify polygons",
            "type": "python",
            "request": "launch",
            "module": "geodense.main",
            "args": [
                "densify",
                "${workspaceFolder}/tests/data/polygons.json",
                "${workspaceFolder}/out-data/out.json",
                "--max-segment-length",
                "5000",
                "-o"
            ],
            "justMyCode": false
        },
        {
            "name": "Python: densify linestring 3d",
            "type": "python",
            "request": "launch",
            "module": "geodense.main",
            "args": [
                "densify",
                "${workspaceFolder}/tests/data/linestrings_3d.json",
                "${workspaceFolder}/out-data/foobar/out.json",
                "--max-segment-length",
                "5000",
                "-o"
            ],
            "justMyCode": false,
        },
        {
            "name": "Python: check density polygons",
            "type": "python",
            "request": "launch",
            "module": "geodense.main",
            "args": [
                "check-density",
                "${workspaceFolder}/tests/data/feature-geometry-collection-with-point.json",
                "--max-segment-length",
                "5000",
                "--src-crs",
                "EPSG:28992",
                "-r",
                "${workspaceFolder}/foobar/report.json",
                "-o"
            ],
            "justMyCode": false
        },
        {
            "name": "Python: Debug Tests",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "purpose": ["debug-test"],
            "console": "integratedTerminal",
            "justMyCode": false
          }
    ]
}
